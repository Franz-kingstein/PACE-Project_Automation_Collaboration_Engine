# Copy this file to .env in the same folder and fill in values
# Firebase configuration is already set up in firebase.js

# Google AI API Key for AI Chatbot feature
REACT_APP_GOOGLE_API_KEY=your_google_ai_api_key_here

# Firebase Configuration (these are safe to be public but you can override)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Optional: Custom chat API endpoint (if you want to use your own proxy)
# REACT_APP_CHAT_API_URL=https://your-domain.com/api/chat
